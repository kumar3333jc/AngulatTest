{
  "name": "angulartest2",
  "version": "1.1.3",
  "main": "entry.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kumar3333jc/AngulatTest.git"
  },
  "build": {
    "appId": "angulartest2",
    "productName": "angulartest2",
    "win": {
      "asar": false,
      "target": [
        {
          "target": "portable",
          "arch": [
            "ia32"
          ]
        }
      ],
      "publish": {
        "provider": "github",
        "owner": "kumar3333jc",
        "repo": "AngulatTest"
      },
      "icon": "src/assets/images/Preview.jpg"
    },
    "directories": {
      "output": "exefolder"
    },
    "files": [
      "!src"
    ]
  },
  "publish": {
    "provider": "github",
    "owner": "kumar3333jc",
    "repo": "AngulatTest"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "install-app-deps",
    "run": "ng build --prod --base-href ./ && electron .",
    "pack": "ng build --prod --base-href ./ && electron-builder build  --win --publish always",
    "deploy": " electron-builder build   --win --publish always"
  },
  "dependencies": {
    "3": "^2.1.0",
    "@angular/animations": "~8.2.11",
    "@angular/common": "~8.2.11",
    "@angular/compiler": "~8.2.11",
    "@angular/core": "~8.2.11",
    "@angular/forms": "~8.2.11",
    "@angular/platform-browser": "~8.2.11",
    "@angular/platform-browser-dynamic": "~8.2.11",
    "@angular/router": "~8.2.11",
    "electron-updater": "^4.0.6",
    "knex": "^0.20.4",
    "rxjs": "~6.4.0",
    "sqlite": "^3.0.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.10",
    "@angular/cli": "~8.3.10",
    "@angular/compiler-cli": "~8.2.11",
    "@angular/language-service": "~8.2.11",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "electron": "^15.5.5",
    "electron-builder": "^22.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
